<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net7.0;net8.0;net9.0</TargetFrameworks>
    <LangVersion>13.0</LangVersion>
    <AssemblyName>Skyware.ErpNetFS.Model</AssemblyName>
    <RootNamespace>Skyware.ErpNetFS.Model</RootNamespace>
    <Authors>Mikhail Kalatchev</Authors>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Product>ErpNet.FS Fiscal server model</Product>
    <Description>Data Model for ErpNet.FS Fiscal server.</Description>
    <Company>SKYWARE Group</Company>
    <Copyright>Copyright (C) 2022-2025, SKYWARE Group</Copyright>
    <PackageIcon>erp-net-fs-client.png</PackageIcon>
    <PackageReadmeFile>erp-net-fp-client-nuget-readme.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Fiscal server;Datecs;Tremol;Daisy;Eltrade;Incotex;ISL;SKYWARE Group;ErpNet.FS</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/SKYWARE-Group/ErpNetClient</RepositoryUrl>
    <Version>0.4.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Assets\erp-net-fp-client-nuget-readme.md">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </Content>
    <Content Include="Assets\erp-net-fs-client.png">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="9.0.4" />
  </ItemGroup>

</Project>
